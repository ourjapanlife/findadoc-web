{
    "name": "findadoc-web",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "prepare": "husky",
        "dev": "yarn && nuxi dev",
        "dev:localserver": "NUXT_USE_LOCAL_API=true; yarn && nuxi dev",
        "generate": "graphql-codegen --config ./typesgeneratorconfig.ts",
        "lint": "ESLINT_USE_FLAT_CONFIG=true eslint . -c eslint.config.js",
        "lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint . -c eslint.config.js --fix",
        "lint:locales": "node i18n/checkLocaleKeys.js",
        "lint:cleanlocales": "node i18n/cleanUnusedLocaleKeys.js",
        "lint:ci": "yarn prepare:ci && eslint . --config eslint.config.js --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",
        "prod:build": "yarn && yarn generate && nuxi generate",
        "prod:start": "nuxi start",
        "ci:build": "yarn && yarn generate && nuxi generate",
        "ci:start": "nuxi start",
        "prepare:ci": "nuxi prepare",
        "test": "yarn test:pinia --run && yarn test:e2e:headless",
        "test:pinia": "nuxi prepare && nuxi generate && vitest",
        "test:pinia:coverage": "vitest run --coverage",
        "test:e2e": "NUXT_IS_TESTING_MODE=true yarn dev:localserver & npx wait-on http://localhost:3000 && cypress open",
        "test:e2e:headless": "NUXT_IS_TESTING_MODE=true yarn dev:localserver & npx wait-on http://localhost:3000 && cypress run --e2e",
        "test:e2e:ci": "cypress run --e2e",
        "test:snapshot": "jest --config jest.config.cjs",
        "test:snapshot:update": "jest --updateSnapshot"
    },
    "packageManager": "yarn@4.4.0",
    "engines": {
        "node": ">=21.3.0"
    },
    "type": "module",
    "dependencies": {
        "@auth0/auth0-vue": "^2.3.3",
        "@pinia/nuxt": "^0.5.2",
        "@vueform/multiselect": "^2.6.11",
        "@webzlodimir/vue-bottom-sheet": "^3.0.5",
        "cypress-real-events": "^1.13.0",
        "eslint-plugin-jest": "^28.12.0",
        "graphql": "^16.9.0",
        "graphql-request": "^7.1.0",
        "hammerjs": "^2.0.8",
        "nuxt-svgo": "^4.0.2",
        "pinia": "^2.2.0",
        "vue": "^3.4.35",
        "vue-gtag": "^2.0.1",
        "vue-router": "^4.4.2",
        "vue-server-renderer": "^2.7.16",
        "vue-template-compiler": "^2.7.16",
        "vue-toastification": "^2.0.0-rc.5",
        "vue3-google-map": "^0.21.0"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^5.0.3",
        "@graphql-codegen/typescript": "^4.0.9",
        "@graphql-codegen/typescript-resolvers": "^4.2.1",
        "@jest/globals": "30.0.0-beta.3",
        "@microsoft/eslint-formatter-sarif": "^3.1.0",
        "@nuxt/eslint": "^0.5.0",
        "@nuxt/test-utils": "^3.14.3",
        "@nuxt/types": "^2.18.1",
        "@nuxtjs/i18n": "^9.1.0",
        "@stylistic/eslint-plugin": "^2.6.1",
        "@swc/core": "^1.7.6",
        "@testing-library/vue": "^8.1.0",
        "@types/jest": "^29.5.14",
        "@types/hammerjs": "^2",
        "@vitejs/plugin-vue": "^5.1.2",
        "@vitest/coverage-v8": "^2.1.3",
        "@vitest/eslint-plugin": "^1.1.27",
        "@vue/test-utils": "^2.4.6",
        "@vue/vue3-jest": "^29.2.6",
        "all-contributors-cli": "^6.26.1",
        "autoprefixer": "^10.4.20",
        "cypress": "^13.13.2",
        "cypress-plugin-tab": "^1.0.5",
        "dotenv-cli": "^8.0.0",
        "eslint": "^9.8.0",
        "eslint-plugin-cypress": "^3.4.0",
        "eslint-plugin-json": "^4.0.1",
        "eslint-plugin-vue": "^9.27.0",
        "husky": "^9.1.4",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "30.0.0-beta.3",
        "jest-serializer-vue": "^3.1.0",
        "jest-transform-stub": "^2.0.0",
        "jsdom": "^24.1.1",
        "nuxt": "^3.15.2",
        "nuxt-gtag": "3.0.2",
        "nuxt-viewport": "^2.1.5",
        "postcss": "^8.4.41",
        "postcss-html": "^1.7.0",
        "start-server-and-test": "^2.0.8",
        "tailwindcss": "^3.4.7",
        "ts-jest": "^29.3.4",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.0.1",
        "vite": "^5.3.5",
        "vitest": "^2.1.3",
        "webpack": "^5.93.0"
    }
}
