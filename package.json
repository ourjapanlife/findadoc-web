{
    "name": "findadoc-web",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "prepare": "husky install",
        "dev": "yarn && nuxi dev",
        "generate": "graphql-codegen --config ./typesgeneratorconfig.ts",
        "lint": "ESLINT_USE_FLAT_CONFIG=true eslint . -c eslint.config.js && stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
        "lint:ci": "eslint . --config eslint.config.js --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",
        "lint:css": "stylelint '**/*.{vue,css,scss}' --ignore-path .gitignore -f verbose",
        "prod:build": "yarn && yarn generate && nuxi generate",
        "prod:start": "nuxi start",
        "prepare:ci": "nuxi prepare",
        "test:pinia": "vitest",
        "test:pinia:coverage": "vitest run --coverage",
        "test:e2e": "cypress run --e2e --browser chrome"
    },
    "packageManager": "yarn@4.1.1",
    "engines": {
        "node": ">=21.3.0"
    },
    "type": "module",
    "dependencies": {
        "@pinia/nuxt": "^0.5.1",
        "graphql": "^16.8.1",
        "graphql-request": "^6.1.0",
        "pinia": "^2.1.7",
        "typescript-eslint": "^7.2.0",
        "vue": "^3.4.21",
        "vue-gtag": "^2.0.1",
        "vue-router": "^4.3.0",
        "vue-server-renderer": "^2.7.16",
        "vue-template-compiler": "^2.7.16",
        "vue3-google-map": "^0.19.0"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^5.0.2",
        "@graphql-codegen/typescript": "^4.0.6",
        "@graphql-codegen/typescript-resolvers": "^4.0.6",
        "@microsoft/eslint-formatter-sarif": "^3.0.0",
        "@nuxt/types": "^2.17.3",
        "@nuxtjs/i18n": "next",
        "@stylistic/eslint-plugin": "^1.7.0",
        "@swc/core": "^1.4.8",
        "@testing-library/vue": "^8.0.3",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vitest/coverage-v8": "^1.4.0",
        "@vue/test-utils": "^2.4.5",
        "all-contributors-cli": "^6.26.1",
        "autoprefixer": "^10.4.18",
        "cypress": "^13.7.0",
        "eslint": "^8.57.0",
        "eslint-plugin-cypress": "^2.15.1",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-vue": "^9.23.0",
        "husky": "^9.0.11",
        "jsdom": "^24.0.0",
        "nuxt": "^3.11.0",
        "nuxt-viewport": "^2.1.4",
        "postcss": "^8.4.36",
        "postcss-html": "^1.6.0",
        "start-server-and-test": "^2.0.3",
        "stylelint": "^16.2.1",
        "stylelint-config-recommended-vue": "^1.5.0",
        "stylelint-config-standard-scss": "^13.0.0",
        "stylelint-order": "^6.0.4",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.4.2",
        "vitest": "^1.4.0",
        "webpack": "^5.90.3"
    }
}
