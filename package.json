{
    "name": "findadoc-web",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "prepare": "husky",
        "dev": "yarn && nuxi dev",
        "dev:localserver": "NUXT_USE_LOCAL_API=true; yarn && nuxi dev",
        "generate": "graphql-codegen --config ./typesgeneratorconfig.ts",
        "lint": "ESLINT_USE_FLAT_CONFIG=true eslint . -c eslint.config.js",
        "lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint . -c eslint.config.js --fix",
        "lint:locales": "node i18n/checkLocaleKeys.js",
        "lint:cleanlocales": "node i18n/cleanUnusedLocaleKeys.js",
        "lint:ci": "yarn prepare:ci && eslint . --config eslint.config.js --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",
        "prod:build": "yarn && yarn generate && nuxi generate",
        "prod:start": "nuxi start",
        "ci:build": "yarn && yarn generate && nuxi generate",
        "ci:start": "nuxi start",
        "prepare:ci": "nuxi prepare",
        "test": "yarn test:pinia --run && yarn test:e2e:headless",
        "test:pinia": "nuxi prepare && nuxi generate && vitest",
        "test:pinia:coverage": "vitest run --coverage",
        "test:e2e": "NUXT_IS_TESTING_MODE=true yarn dev:localserver & npx wait-on http://localhost:3000 && cypress open",
        "test:e2e:headless": "NUXT_IS_TESTING_MODE=true yarn dev:localserver & npx wait-on http://localhost:3000 && cypress run --e2e",
        "test:e2e:ci": "cypress run --e2e",
        "test:snapshot": "vitest run tests/vitest/snapshotTests",
        "test:snapshot:update": "vitest run tests/vitest/snapshotTests --update"
    },
    "packageManager": "yarn@4.11.0",
    "engines": {
        "node": ">=21.3.0"
    },
    "type": "module",
    "dependencies": {
        "@auth0/auth0-vue": "^2.4.0",
        "@pinia/nuxt": "^0.11.3",
        "cypress-real-events": "^1.15.0",
        "graphql": "^16.12.0",
        "graphql-request": "^7.3.4",
        "hammerjs": "^2.0.8",
        "nuxt-svgo": "^4.2.6",
        "pinia": "^3.0.4",
        "vue": "^3.5.24",
        "vue-gtag": "^3.6.3",
        "vue-router": "^4.6.3",
        "vue-server-renderer": "^2.7.16",
        "vue-template-compiler": "^2.7.16",
        "vue-toastification": "^1.7.14",
        "vue3-google-map": "^0.25.1"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^6.0.2",
        "@graphql-codegen/typescript": "^5.0.4",
        "@graphql-codegen/typescript-resolvers": "^5.1.2",
        "@microsoft/eslint-formatter-sarif": "^3.1.0",
        "@nuxt/eslint": "^1.10.0",
        "@nuxt/test-utils": "^3.20.1",
        "@nuxt/types": "^2.18.1",
        "@nuxtjs/i18n": "^10.2.1",
        "@pinia/testing": "^1.0.3",
        "@stylistic/eslint-plugin": "^5.6.1",
        "@swc/core": "^1.15.2",
        "@testing-library/vue": "^8.1.0",
        "@types/google.maps": "^3.58.1",
        "@types/hammerjs": "^2.0.46",
        "@vitejs/plugin-vue": "^6.0.2",
        "@vitest/coverage-v8": "^4.0.11",
        "@vitest/eslint-plugin": "^1.4.3",
        "@vue/test-utils": "^2.4.6",
        "all-contributors-cli": "^6.26.1",
        "autoprefixer": "^10.4.22",
        "cypress": "^15.7.0",
        "cypress-plugin-tab": "^1.0.5",
        "dotenv-cli": "^11.0.0",
        "eslint": "^9.39.1",
        "eslint-plugin-cypress": "^5.2.0",
        "eslint-plugin-json": "^4.0.1",
        "eslint-plugin-vue": "^10.5.1",
        "husky": "^9.1.7",
        "jsdom": "^27.2.0",
        "nuxt": "^4.2.1",
        "nuxt-gtag": "^4.1.0",
        "nuxt-viewport": "^2.3.1",
        "postcss": "^8.5.6",
        "postcss-html": "^1.8.0",
        "start-server-and-test": "^2.1.3",
        "tailwindcss": "^4.1.17",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.47.0",
        "vite": "^7.2.4",
        "vitest": "^4.0.11",
        "webpack": "^5.103.0"
    }
}
